/* mayi_logistics/pages/complaint/complaint.wxss */
.container{
  width: 100%;
}
.main{
  width: 95%;
  border: 1rpx solid #ccc;
  margin: 10rpx auto;
}
.btn{
   width: 92%; 
  background-color: #0089d7;
  color: white;
  position: fixed;
  bottom:0;
  left: 4%;
   z-index: 990;
}
.upimg{
  border: 1px dashed #a1a1a1;
   margin: 10px 10px 15px 5px;  
  height: 130rpx;
  border-radius: 10rpx;
  width: 130rpx;
  font-size: 130rpx;
  color: #ccc;
  text-align: center;
  font-weight: 100;
  line-height: 130rpx;
}
.clearfix:after{
    content: ".";
  height: 0;
  line-height: 0;
  display: block;
  visibility: hidden;
  clear: both;
}
.addImg{
  display: block;
 padding-top: 60rpx;
 color: #ccc;
}
